{
    "name": "ReactNativeTS",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "start:ios": "react-native run-ios",
        "linting": "gulp tslint",
        "test": "jest"
    },
    "dependencies": {
        "@types/axios": "^0.14.0",
        "@types/react": "^16.0.36",
        "@types/react-native": "^0.52.7",
        "@types/react-navigation": "^1.5.2",
        "@types/react-redux": "^5.0.15",
        "@types/redux": "^3.6.0",
        "@types/redux-thunk": "^2.1.0",
        "axios": "^0.18.0",
        "react": "^16.2.0",
        "react-native": "^0.52.2",
        "react-navigation": "^1.5.8",
        "react-redux": "^5.0.6",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "tslib": "^1.9.0"
    },
    "devDependencies": {
        "@types/jest": "^22.1.1",
        "@types/react-test-renderer": "^16.0.0",
        "babel-jest": "^22.1.0",
        "babel-preset-react-native": "^4.0.0",
        "gulp": "^3.9.1",
        "gulp-tslint": "^8.1.2",
        "jest": "22.1.4",
        "jest-react-native": "^18.0.0",
        "react-dom": "^16.2.0",
        "react-native-typescript-transformer": "^1.2.3",
        "react-test-renderer": "^16.2.0",
        "require-dir": "^0.3.2",
        "ts-jest": "^22.0.2",
        "tslint": "^5.9.1",
        "typescript": "^2.7.1"
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "useBabelrc": true
            }
        },
        "preset": "react-native",
        "transform": {
            "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    }
}
